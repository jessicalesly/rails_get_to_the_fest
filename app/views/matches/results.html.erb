<!-- Beginning of the filtered menu -->

  <div class= "searchbar-icon" id= "searchicon">
    <div class="background-filter" id="search-filter">
      <%= form_tag match_path_url, method: :get, class: 'form-inline' do %>
        <%= text_field_tag :start_date,
            @start_date,
            class: "form-control filter_style", id: "range_start",
            placeholder: "Start at" %>
        <%= text_field_tag :end_date,
            @end_date,
            class: "form-control filter_style", id: "secondRangeInput",
            placeholder: "End at" %>
      <%= text_field_tag :localisation,
        params[:localisation],
        class: "form-control filter_style", id: "fest_address",
        placeholder: "Localisation" %>
   <!--    <#%= text_field_tag :query,
        params[:query],
        class: "form-control filter_style",
        placeholder: "Genre" %> -->
      <%= text_field_tag :artist,
        params[:artist],
        class: "form-control filter_style",
        placeholder: "Artiste" %>
      <%= submit_tag 'Rechercher', class: 'button-search' %>
    <% end %>
  </div>
  </div>
    <div  id= "nav-icon2">
        <i class="fas fa-search"></i>
    </div>
<!-- End of the filtered menu -->

<!-- Results -->
<div class="yellow-container-results">
  <div class="container">
    <div class="row">

      <% counter = 0 %>
      <% animal_counter = 0 %>
      <% @festival_array.each do |festival_hash|  %>
        <% counter += 1 %>
        <div class="col-md-8 col-md-offset-2">
          <div class="card-black">
            <!-- Front of the card -->
            <div class="front">
              <div class="pad">
                <h2 id="title-fest"><%= festival_hash[:festival_instance].name %></h2>
                <div id="title-place">
                  <div class="flex-align-colle">
                    <h2 id="city"><%= festival_hash[:festival_instance].city %></h2>
                    <h2 class="text-bold" id="country">/ <%= festival_hash[:festival_instance].country.upcase %></h2>
                  </div>
                </div>
                <div class="title-affinity">
                  <h2 style="font-size: 40px"><%= festival_hash[:affinity] %>%</h2>d'affinité
                </div>
                <div class="card-date">
                  <h3 class="month">AOUT</h3>
                  <h2 class="day">12</h2>
                  <h3 style="color:white; margin:0; font-weight:bold">>></h3>
                  <h3 class ="month">AOUT</h3>
                  <h2 class="day">12</h2>
                </div>
                <div class="btn-turn">
                  <div class="btn-get-turn"><p>Line Up</p></div>
                </div>
              </div>
            </div>
            <!-- Back of the card -->
            <div class="back">
              <p style="color: white;">BONJOUR</p>
              <div>
                <iframe src=<%"#{festival_hash[:festival_instance].playlist}"%> width="100" height="100" frameborder="0" allowtransparency="true" allow="encrypted-media" style="padding: 10px;"></iframe>
              </div>
            </div>
            <div class="line-up line-up-close">
              <div class="line-up-content line-up-content-close">
                <ul>
                  <% related = [] %>
                  <% other_artists = [] %>
                  <% festival_hash[:artists].each do |artist| %>
                    <% if artist[:score] > 1 %>
                      <li>
                        <img src=<%="#{Artist.find_by(name: artist[:name]).picture}"%> style="height: 50px; width: 50px;" alt="">
                        <%= artist[:name] %>
                        <%= ", is one of your top artists" if artist[:is_top_artist] %>
                        <%= ", we found #{artist[:nb_top_tracks]} tracks in your top tracks" if artist[:nb_top_tracks] > 0 %>
                        <%= ", you saved #{artist[:nb_saved_tracks]} tracks" if artist[:nb_saved_tracks] > 0 %>
                        <%= "is an artist you could like" if artist[:score] == 1 %>
                      </li>
                    <% elsif artist[:score] > 0 %>
                      <% related << artist[:name] %>
                    <% else %>
                      <% other_artists << artist[:name] %>
                    <% end %>
                  <% end %>
                  <li>
                    <%= "#{related.count} artists you could like : #{related.join(', ')}" %>
                  </li>
                  <li>
                    <%= "Other artists : #{other_artists.join(', ')}" %>
                  </li>
                </ul>
              </div>
              <div class="btn-line-up">
                  <div class="btn-get-line-up">Get back to the Fest</div>
              </div>
            </div>
          </div>
        </div>

        <!-- End of the festival card -->

        <!-- Renard logo -->
        <% if counter%2 == 0 %>
          <% animal_counter +=1 %>
          <% if animal_counter%2 == 0 %>
            <div class="col-md-6 col-md-offset-3">
              <img src="<%=image_path('renard.png')%>" style="margin-top:90px;" alt="">
            </div>
          <% else %>
            <div class="col-md-4 col-md-offset-4">
              <img src="<%=image_path('suricate.png')%>" style="margin-top:90px;" alt="">
            </div>
          <% end %>
        <% end %>
      <% end %>

    </div>
  </div>
</div>

